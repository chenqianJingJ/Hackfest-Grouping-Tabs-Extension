{
    "manifest_version": 3,
    "name": "Tab Grouping Robot",
    "version": "1.0",
    "description": "Chrome extension for automatically grouping tabs using TypeScript",
    "permissions": [
        "tabs"
    ],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ],
    "action": {
        "default_icon": "cat-tabs.png",
        "default_popup": "hello.html"
    }
}