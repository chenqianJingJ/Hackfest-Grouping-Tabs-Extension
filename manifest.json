{
    "manifest_version": 3,
    "name": "Tab Grouping Helper",
    "version": "1.0",
    "description": "Chrome extension for automatically grouping tabs using TypeScript",
    "permissions": [
        "tabs",
        "tabGroups"
    ],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ],
    "action": {
        "default_icon": "assets/tabsGroupingCat.png",
        "default_popup": "smallWindow.html"
    }
}